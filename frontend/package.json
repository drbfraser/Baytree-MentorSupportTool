  "scripts": {
    "start": "vite --host --port 3000",
    "build": "tsc && vite build",
    "format": "prettier --write src/**/*.ts{,x}",
    "cy:open": "cypress open",
    "cy:headless": "cypress run --quiet",
    "test": "start-server-and-test http-get://localhost:3000 cy:open",
    "coverage": "nyc report",
    "coverage:html": "open coverage/lcov-report/index.html",
    "test:component": "cypress run --component --quiet",
