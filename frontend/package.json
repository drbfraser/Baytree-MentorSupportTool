{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "coverage": "nyc report",
    "coverage:html": "open coverage/lcov-report/index.html",
    "cy:headless": "cypress run --quiet --config watchForFileChanges=false",
    "cy:open": "cypress open",
    "format": "prettier --write src/**/*.{ts,tsx} cypress/**/*.{ts,tsx}",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "start": "vite --host --port 3000",
    "test": "start-server-and-test http-get://localhost:3000 cy:open",
    "test:component": "cypress run --component --quiet",
    "test:e2e": "start-server-and-test http-get://localhost:3000 cy:headless",
    "test:jest": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "exclude": [
      "public",
      "cypress",
      "src/mocks"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fullcalendar/daygrid": "^5.11.2",
    "@fullcalendar/interaction": "^5.11.2",
    "@fullcalendar/react": "^5.11.2",
    "@fullcalendar/rrule": "^5.11.2",
    "@fullcalendar/timegrid": "^5.11.2",
    "@mui/lab": "^5.0.0-alpha.95",
    "@mui/material": "^5.10.1",
    "@mui/x-data-grid": "5.15.3",
    "@mui/x-date-pickers": "^5.0.0-beta.4",
    "axios": "^0.27.2",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.6",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.3",
    "react-icons": "^4.4.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@cypress/code-coverage": "^3.10.0",
    "@testing-library/cypress": "^8.0.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^1.3.2",
    "axios-mock-adapter": "^1.21.2",
    "babel-jest": "^29.3.1",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.6.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "jest": "^29.3.1",
    "msw": "^0.47.4",
    "prettier": "^2.8.3",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
