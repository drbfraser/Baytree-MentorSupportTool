{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fullcalendar/daygrid": "^5.11.0",
    "@fullcalendar/interaction": "^5.11.0",
    "@fullcalendar/react": "^5.11.1",
    "@fullcalendar/rrule": "^5.11.0",
    "@fullcalendar/timegrid": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.56",
    "@mui/material": "^5.0.3",
    "@mui/x-data-grid": "5.2.0",
    "@mui/x-date-pickers": "^5.0.0-beta.4",
    "axios": "^0.27.2",
    "date-fns": "^2.25.0",
    "date-fns-tz": "^1.3.5",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^8.2.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "vite --host --port 3000",
    "build": "tsc && vite build",
    "format": "prettier --write src/**/*.ts{,x}",
    "cy:open": "cypress open",
    "cy:headless": "cypress run --headless",
    "test": "start-server-and-test http-get://localhost:3000 cy:open",
    "coverage": "nyc report",
    "coverage:html": "open coverage/lcov-report/index.html",
    "test:component": "cypress run --component --headless",
    "test:e2e": "start-server-and-test http-get://localhost:3000 cy:headless",
    "test:component:ci": "npm run test:component && npm run coverage",
    "test:e2e:ci": "npm run test:e2e && npm run coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@testing-library/cypress": "^8.0.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.5.0",
    "msw": "^0.44.2",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "nyc": {
    "exclude": [
      "public",
      "cypress",
      "src/mocks"
    ]
  }
}
